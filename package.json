{
  "name": "excel-cource",
  "version": "1.0.0",
  "description": "Pure JS excel app",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "prod": "cross-env NODE_ENV=production webpack",
    "lint": "eslint src --ext js",
    "lint:fix": "eslint --fix --ext .js src",
    "lint:staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greed2kk/excel_practice.git"
  },
  "keywords": [
    "excel",
    "js"
  ],
  "author": "Greed2kk <draiver78@mail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Greed2kk/excel_practice/issues"
  },
  "homepage": "https://github.com/Greed2kk/excel_practice#readme",
  "private": true,
  "browserslist": ">0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,json}": [
      "prettier --list-different"
    ]
  },
  "babel": {
    "presets": [
      [
        "airbnb",
        {
          "targets": {
            "node": "15.1.0"
          }
        },
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^5.0.0",
    "prettier": "2.1.2",
    "sass-loader": "^10.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.1"
  }
}
